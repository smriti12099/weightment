{
  "_version": "1.65.0",
  "sap.app": {
    "id": "weighment",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.16.3",
      "toolsId": "9208acbd-a60d-4d3f-ad59-5fb3bc4c496e"
    },
    "dataSources": {
      "ZGE_SB_HEAD_WT_SERVICE_V2_VAN": {
        "uri": "/sap/opu/odata/IWFND/CATALOGSERVICE;v=2/Annotations(TechnicalName='ZGE_SB_HEAD_WT_SERVICE_V2_VAN',Version='0001')/$value/",
        "type": "ODataAnnotation",
        "settings": {
          "localUri": "localService/mainService/ZGE_SB_HEAD_WT_SERVICE_V2_VAN.xml"
        }
      },
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZGE_SB_HEAD_WT_SERVICE_V2/",
        "type": "OData",
        "settings": {
          "annotations": [
            "ZGE_SB_HEAD_WT_SERVICE_V2_VAN"
          ],
          "localUri": "localService/mainService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "zsemobj_wt-manage": {
          "semanticObject": "zsemobj_wt",
          "action": "manage",
          "title": "{{flpTitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        },
        "gate_entry_so-DISPLAY": {
          "semanticObject": "gate_entry_so",
          "action": "DISPLAY",
          "title": "{{gate_entry_so-DISPLAY.flpTitle}}",
          "subTitle": "{{gate_entry_so-DISPLAY.flpSubtitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.130.5",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "weighment.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "weighment.view",
        "async": true,
        "viewPath": "weighment.view"
      },
      "routes": [
        {
          "name": "RouteWeighment",
          "pattern": ":?query:",
          "target": [
            "TargetWeighment"
          ]
        },
        {
          "name": "RouteWeighmentDetails",
          "pattern": "Details/{GateNumber}/:WeighmentType:/:Edit:",
          "target": [
            "TargetWeighmentDetails"
          ]
        }
      ],
      "targets": {
        "TargetWeighment": {
          "id": "Weighment",
          "name": "Weighment"
        },
        "TargetWeighmentDetails": {
          "id": "Details",
          "name": "Details"
        }
      }
    },
    "rootView": {
      "viewName": "weighment.view.App",
      "type": "XML",
      "id": "App"
    }
  },
  "sap.cloud": {
    "public": true,
    "service": "weighment"
  }
}
