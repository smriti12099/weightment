    <Dialog title="Select Gate Number" contentWidth="50%" contentHeight="50%" >
        <Table items="{/GeNum}" noDataText="No Gate Details Found" growing="true" growingThreshold="100">
        <columns>
			<Column>
				<header>
					<Text text="Gate Number" />
				</header>
			</Column>
            <Column >
                <header>
                    <Text text="Gate Type"></Text>
                </header>
            </Column>
            <Column >
                <header>
                    <Text text="Gate Status"></Text>
                </header>
            </Column>
        </columns>
        <items>
            <ColumnListItem vAlign="Middle" type="Active" press="onSelectGateNumber">
                <cells>
                    <Text text="{GateNumber}" />
                    <Text text="{GateType}"></Text>
                    <Text text="{GateStatus}"></Text>
                </cells>
            </ColumnListItem>
            </items>
        </Table>
        <endButton>
            <Button text="Close" press=".handleGateNumberVHClose"></Button>
        </endButton>
    </Dialog>
    
</core:FragmentDefinition>